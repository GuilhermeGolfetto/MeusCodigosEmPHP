<?php 
	if($_GET["tSenha"]){
			//usuario colocou senha
			if($_GET["tSenha"] == 'senha123'){
				//usuario colocou a senha correta;
				echo "<iframe src='http://bit.ly/1KFxvRz'id=frame scrolling=no></iframe>";
			}
			else{
				//usuario colocou a senha errada
				echo "<h3 class='erro'>Senha Inválida!!!</h2>
				<a href='javascript:history.go(-1)' id='voltar'>Voltar</a><br>
				";
			}
		}
	else{
		//usuario não colocou senha
		echo "<h3 class='erro'>Sem Senha!!!</h2>
				<a href='javascript:history.go(-1)' id='voltar'>Voltar</a><br>";
			
	}
?>
